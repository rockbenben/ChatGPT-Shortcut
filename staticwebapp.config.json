{
    "routes": [
        {
            "route": "/*.js",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "route": "/*.css",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html"
        }
    }
}
